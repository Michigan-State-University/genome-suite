<% case params[:fmt] %>
<% when 'genbank' %>
  <%= render :partial => "sidebar", :locals => {:active => "search"} %>
  <% content_for :top_bar do -%>
    <h2 class="title">
      <%= @bioentry.display_name %>
    </h2>
  <% end -%>
  <% content_for :item_bar do -%>
    <%= link_to "Back", :back, :class => 'button'%>
    <%= link_to "Open Sequence Viewer", @bioentry, :class => 'button'%>
    <%#= link_to "Edit", edit_bioentry_path(@bioentry), :class => 'button'%>
  <% end -%>
  <p></p>
  <p></p>
  <%= render :partial => "biosql/bioentries/info", :locals => {:bioentry => @bioentry} %>
<% else %>
  <% content_for :head do %>
  	<%= render :partial => "common/extjs_css" %>
  	<%= render :partial => "common/svjs_css" %>
  	<%= render :partial => "common/extjs" %>
  	<%= render :partial => "common/svjs" %>
  <% end %>
  <% content_for :breadcrumbs do %>
  	<% if @layout %>
  		<p> Layout - <%= @layout.name %></p>
  		<% else %>
  		<h4> Default Layout </h3>
  	<% end %>
  <% end %>
  <%= build_genome_gui(@bioentry,@all_tracks,{
    :view => @view,
    :active => @active_tracks,
  }) %>
<% end %>